; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[addons]

resources_spreadsheet_view/array_color_tint=100.0
resources_spreadsheet_view/color_rows=true
resources_spreadsheet_view/array_min_width=128.0
resources_spreadsheet_view/resource_preview_size=32.0
resources_spreadsheet_view/clip_headers=false
resources_spreadsheet_view/dupe_arrays=true
resources_spreadsheet_view/context_menu_on_leftclick=false
resources_spreadsheet_view/fold_docks=true
resources_spreadsheet_view/resource_cell_label_mode=1
resources_spreadsheet_view/freeze_first_column=true

[application]

config/name="TRPG"
run/main_scene="res://src/core/managers/level/level.tscn"
config/features=PackedStringArray("4.4", "Forward Plus")
config/icon="res://src/assets/icon.svg"

[autoload]

LayerNames="*res://addons/layerNames/layerNames.gd"
EventBus="*res://src/core/global/event_bus.gd"

[debug]

gdscript/warnings/integer_division=0

[display]

window/size/viewport_width=495
window/size/viewport_height=270
window/size/always_on_top=true
window/size/window_width_override=1920
window/size/window_height_override=1080
window/stretch/mode="canvas_items"
mouse_cursor/custom_image="uid://d1rbipymj4tck"

[editor_plugins]

enabled=PackedStringArray("res://addons/layerNames/plugin.cfg", "res://addons/resources_spreadsheet_view/plugin.cfg")

[file_customization]

folder_colors={
"res://src/assets/": "red",
"res://src/core/": "purple",
"res://src/data/": "yellow",
"res://src/entities/": "orange",
"res://src/ui/": "blue",
"res://src/world/": "green",
"res://tests/": "gray"
}

